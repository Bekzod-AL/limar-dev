    "165342933895b24228": {
      "type": "advanced-content",
      "blocks": {
        "e7738003-aa55-4a69-a23f-4c68578a9f1a": {
          "type": "liquid",
          "settings": {
            "code": "<div style=\"display:block; height: 50px;\"><\/div>",
