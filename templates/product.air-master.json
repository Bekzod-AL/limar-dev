          }
        },
        "b4d6c064-0fd7-4860-b748-c42e83a423e5": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/airstar-emotion-1_dafc01f4-32b9-48ff-9671-73c4265cd5a9.jpg",
