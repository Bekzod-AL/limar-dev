    "165342933895b24228": {
      "type": "advanced-content",
      "blocks": {
        "b4d6c064-0fd7-4860-b748-c42e83a423e5": {
          "type": "image",
          "settings": {
            "image": "shopify://shop_images/pora-emotion-3.jpg",
