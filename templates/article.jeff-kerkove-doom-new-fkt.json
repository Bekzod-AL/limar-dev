    "c647556d-5166-4f53-9f29-d0489662d962": {
      "type": "advanced-content",
      "blocks": {
        "template--16779547050199__c647556d-5166-4f53-9f29-d0489662d962-16844494714e5628e1-1": {
          "type": "liquid",
          "settings": {
            "code": "<h2>up and down</h2>\n\n<p>Doom is aptly named. For the longest time, no one completed the full route. And still to this day more people fail than complete the full 390-mile loop. Choose your equipment wisely. Come ready for the vision quest.</p>",
            "width": "100%",
            "alignment": "center"
          }
        },
        "template--16779547050199__c647556d-5166-4f53-9f29-d0489662d962-16844494714e5628e1-0": {
          "type": "image",
          "settings": {
            "image": "shopify://shop_images/41023-DOOM-23.jpg",
            "link": "",
            "width": "100%",
            "alignment": "center"
          }
        }
      },
      "block_order": [
        "template--16779547050199__c647556d-5166-4f53-9f29-d0489662d962-16844494714e5628e1-1",
