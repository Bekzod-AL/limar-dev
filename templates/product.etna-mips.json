    "3f69e618-48f8-4f5f-beb5-e55a1179a814": {
      "type": "advanced-content",
      "blocks": {
        "template--16770230419671__3f69e618-48f8-4f5f-beb5-e55a1179a814-16830862560c1edf26-1": {
          "type": "liquid",
          "settings": {
