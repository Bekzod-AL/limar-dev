        "ct_tech_specs": "<table>\n<tbody>\n<tr>\n<th>Technology<\/th>\n<td>\n<p> Monoshell in-mould â€“ SUPERLIGHT technology<\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Fit<\/th>\n<td>\n<p>Extended coverage, compact design<\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Pads<\/th>\n<td>\n<p> Non allergenic washable comfort pads <\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Size (cm)<\/th>\n<td>\n<p>M (53-57), L (57-61)<\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Weight (g)<\/th>\n<td>\n<p>Medium - 260gr | Large - 285gr<\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Air Vents<\/th>\n<td>\n<p> 22 air vents <\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Helmet LED Light<\/th>\n<td>\n<p>Available as an accessory<\/p>\n<\/td>\n<\/tr>\n<tr>\n<th>Ideal For<\/th>\n<td>\n<p>All mountain<\/p>\n<\/td>\n<\/tr>\n<\/tbody>\n<\/table>"
      }
