    "901940ce-fe9c-4c18-a3db-1ecc40213764": {
      "type": "advanced-content",
      "blocks": {
        "template--16826808369367__901940ce-fe9c-4c18-a3db-1ecc40213764-image-1": {
          "type": "image",
